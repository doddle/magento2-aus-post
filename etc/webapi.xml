<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/australiapost_returns/product_variants/:sku" method="GET">
        <service class="AustraliaPost\Returns\Api\ProductVariationListInterface" method="getItems"/>
        <resources>
            <resource ref="AustraliaPost_Returns::variations" />
        </resources>
    </route>
</routes>
